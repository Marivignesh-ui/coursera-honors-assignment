{
  "name": "honorsassignment",
  "version": "1.0.0",
  "description": "movie club app",
  "main": "index.js",
  "scripts": {
    "start": "npm run lite",
    "lite": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin images/* --out-dir='dist/images'",
    "usemin": "usemin rating.html -d dist --htmlmin -o dist/rating.html && usemin movies.html -d dist --htmlmin -o dist/movies.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "author": "Marivignesh",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "rimraf": "^2.6.2",
    "uglify-js": "^3.13.10",
    "usemin-cli": "^0.5.1"
  }
}
